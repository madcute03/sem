{
    "name": "sems",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "start": "npm run prod",
        "prod": "php artisan serve --host=0.0.0.0 --port=8000",
        "dev:all": "concurrently \"npm run dev\" \"php artisan serve --port=8000\"",
        "build:prod": "npm run build && php artisan config:cache && php artisan route:cache && php artisan view:cache",
        "setup": "composer install && npm install && cp .env.example .env && php artisan key:generate",
        "lint": "eslint . --ext .js,.jsx --fix",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "test": "php artisan test",
        "migrate": "php artisan migrate",
        "migrate:fresh": "php artisan migrate:fresh --seed",
        "db:seed": "php artisan db:seed",
        "optimize": "php artisan optimize",
        "clear": "php artisan optimize:clear"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.17",
        "@inertiajs/inertia": "^0.11.1",
        "@inertiajs/react": "^1.0.10",
        "@tailwindcss/forms": "^0.5.7",
        "autoprefixer": "^10.4.16",
        "axios": "^1.6.2",
        "dayjs": "^1.11.18",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.31",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "tailwindcss": "^3.3.5",
        "vite": "^5.0.0",
        "ziggy-js": "^2.4.1"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.0.0",
        "eslint": "^8.45.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3"
    }
}
